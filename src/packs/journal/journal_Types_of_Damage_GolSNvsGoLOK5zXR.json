      "_key": "!journal.pages!GolSNvsGoLOK5zXR.VcDmzhfSvGYgPbt5"
    },
    {
      "sort": 700000,
      "name": "Falling",
      "type": "text",
      "_id": "KS7c36S2eHBEC3JM",
      "system": {},
      "title": {
        "show": true,
        "level": 1
      },
      "image": {},
      "text": {
        "format": 1,
        "content": "<p>When any creature falls, they take [[/roll 1D4]] damage per every 10ft.</p>"
      },
      "video": {
        "controls": true,
        "volume": 0.5
      },
      "src": null,
      "ownership": {
