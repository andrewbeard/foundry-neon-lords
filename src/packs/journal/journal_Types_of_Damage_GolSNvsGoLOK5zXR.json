      },
      "_key": "!journal.pages!GolSNvsGoLOK5zXR.JIOk0bbMTlQqzmrU"
    },
    {
      "sort": 250000,
      "name": "Burning",
      "type": "text",
      "_id": "HrWGoJPjxG9QcCRN",
      "system": {},
      "title": {
        "show": true,
        "level": 1
      },
      "image": {},
      "text": {
        "format": 1,
        "content": "<p>If a target is on fire they take damage from the initial burning source and half damage each turn during the Conditions Check Phase until the flame is extinguished.</p>"
      },
      "video": {
        "controls": true,
        "volume": 0.5
      },
      "src": null,
      "ownership": {
