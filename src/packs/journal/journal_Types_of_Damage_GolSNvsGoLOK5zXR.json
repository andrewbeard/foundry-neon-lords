    {
      "sort": 200000,
      "name": "Bleeding",
      "type": "text",
      "_id": "5jpPWVVtjusPhEyZ",
      "system": {},
      "title": {
        "show": true,
        "level": 1
      },
      "image": {},
      "text": {
        "format": 1,
        "content": "<p>Any attack with the Bleeding type damage inflicts a @UUID[Compendium.neon-lords.journal.JournalEntry.Ej0fIoR2umnEbLfo.JournalEntryPage.sk1A640XsLbv0SfC]{Bleeding} condition on the target.</p><p></p>"
      },
      "video": {
        "controls": true,
        "volume": 0.5
      },
      "src": null,
      "ownership": {
        "default": -1,
        "l18XFo7VCP3jkSA6": 3
