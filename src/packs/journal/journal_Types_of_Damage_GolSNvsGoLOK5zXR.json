      "_key": "!journal.pages!GolSNvsGoLOK5zXR.4pTK5o31606ij18g"
    },
    {
      "sort": 900000,
      "name": "Patriotic",
      "type": "text",
      "_id": "u3s2C4sWCZ0ucAyv",
      "system": {},
      "title": {
        "show": true,
        "level": 1
      },
      "image": {},
      "text": {
        "format": 1,
        "content": "<p>An eagle’s screech can be heard every time a patriotic attack lands and deals damage. Patriotic damage also gains an additional bonus equal to the scumdog’s Attitude Mod.</p>"
      },
      "video": {
        "controls": true,
        "volume": 0.5
      },
